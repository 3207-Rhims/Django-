tup = (1,2,4)
print(tup)

tup = (1 , "hi", 1.3)
print(tup)

#nested
tup = (1, [1,2,4],"hi",(1,2,3,4,5,6,67),9,0,9,'hello')

#nested indexing
print(tup[3][-1])

#negative indexing
print(tup[-1][-1])

#slice
print(tup[4:])

print((1,0,1)+(1,0,2))

d={'name':"rownak",'age':23 , "contact":"email"}
print(d['name'])


#tuple value insert

t = (1,2,3,4)
l = list(t)
l.append(5)

new_t = tuple(l)
print(new_t)












